{
  "name": "crypto-keys",
  "version": "0.0.1",
  "description": "Node Javascript Tool to generate ,encrypt and decrypt RSA and EC keys formated as PEM / DER.",
  "keywords": [
    "crypto",
    "pem",
    "der",
    "jwk",
    "spki",
    "pkcs8",
    "asn1",
    "ecc",
    "rsa",
    "jwk thumbprint",
    "keypair"
  ],
  "private": false,
  "homepage": "https://arvati.github.io/crypto-keys",
  "bugs": "https://github.com/arvati/crypto-keys/issues",
  "main": "lib/index.js",
  "scripts": {
    "ok": "node test/mocha",
    "test": "DOTENV_CONFIG_PATH=./test/.env mocha -R spec",
    "docs": "DOTENV_CONFIG_PATH=./test/.env mocha -R markdown > ./docs/tests.md && jsdoc2md ./lib/index.js > ./docs/index.md",
    "git": "git add . && git commit -m",
    "postgit": "git push --all"
  },
  "repository": "github:arvati/crypto-keys",
  "author": "Ademar Arvati Filho (https://github.com/arvati)",
  "license": "MIT",
  "dependencies": {
    "asn1.js": "^5.0.1",
    "des.js": "^1.0.0",
    "elliptic": "^6.4.1",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coverage": "^0.4.0",
    "dotenv": "^8.0.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">7.10.1"
  },
  "files": [
    "./lib/*.js",
    "./docs/*.md",
    "./AUTHORS.md",
    "./test/*.js",
    "./example/*.js"
  ],
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "test": "./test",
    "example": "./example"
  }
}
